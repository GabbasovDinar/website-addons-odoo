<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="assets_editor" inherit_id="website.assets_editor" name="Website Editor">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/website_page_publish/static/src/js/website.editor.js"></script>
            <link rel='stylesheet' href='/website_page_publish/static/src/css/website.editor.css'/>
        </xpath>
    </template>

    <template id="user_navbar" inherit_id="website.user_navbar" groups="base.group_user">
        <xpath expr="//a[@data-action='rename_page']" position="before">
            <li groups="base.group_website_designer"><a href="#" data-action="publish_page">Publish Page</a></li>
        </xpath>
    </template>

</odoo>
